{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "type-check": "tsc --noEmit",
        "type-check:clean": "find src -name '*.d.ts' ! -name 'vite-env.d.ts' -type f -delete 2>/dev/null || true && tsc --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "benchmark": "tsx src/__benchmarks__/index.ts"
    },
    "dependencies": {
        "ace-builds": "^1.43.4",
        "react": "^19.2.0",
        "react-ace": "^14.0.1",
        "react-dom": "^19.2.0",
        "react-router-dom": "^7.9.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "@vitejs/plugin-react": "^5.1.0",
        "@vitest/ui": "^2.1.8",
        "autoprefixer": "^10.4.22",
        "eslint": "^9.39.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "jsdom": "^25.0.1",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.18",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "vite": "^7.2.2",
        "vitest": "^2.1.8"
    }
}
