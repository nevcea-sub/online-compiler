{
    "name": "online-compiler",
    "version": "1.0.0",
    "description": "Online code compiler with multiple language support",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "npx eslint .",
        "format": "npx prettier --write \"**/*.{js,ts,tsx,json,css,html}\"",
        "dev:frontend": "cd frontend && npm run dev",
        "dev:backend": "cd backend && npm run dev",
        "test:frontend": "cd frontend && npm test",
        "test:backend": "cd backend && npm test",
        "benchmark:frontend": "cd frontend && npm run benchmark",
        "benchmark:backend": "cd backend && npm run benchmark",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/node": "^24.10.1",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "prettier": "^3.3.3",
        "tsx": "^4.20.6"
    }
}
